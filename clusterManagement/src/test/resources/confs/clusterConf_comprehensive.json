{
  "region": "us-west-2",
  "spotPriceURL": "http://spot-price.s3.amazonaws.com/spot.js",
  "currency": "USD",
  "key": "data-key",
  "cluster": {
    "name": "italy",
    "subnet": "subnet-id",
    "version": "3.10",
    "serviceRole": "serviceRole",
    "jobFlowRole": "jobFlowRole",
    "logURI": "s3://aws-emr-lg/",
    "masterNode": {
      "count": 1,
      "instanceType": {
        "type": "size",
        "size": "m2.2xlarge"
      },
      "marketType": "on_demand",
      "os": "linux"
    },
    "coreNode": {
      "count": 1,
      "instanceType": {
        "type": "tier",
        "tier": "high"
      },
      "marketType": "spot",
      "os": "linux"
    },
    "installs": [
      "Spark",
      "Hive",
      "HBASE",
      "Hue",
      "Ganglia"
    ],
    "stepConfigs": [
      {
        "name": "Install Hive",
        "useDefault": true,
        "hadoopJarStepConfigs": {}
      }
    ],
    "bootStraps": [
      {
        "name": "Install HBase",
        "script": "s3://elasticmapreduce/bootstrap-actions/setup-hbase"
      }
    ]
  },
  "tiers": {
    "high": [
      {
        "size": "m3.2xlarge",
        "premium": 0.140
      },
      {
        "size": "m3.xlarge",
        "premium": 0.070
      },
      {
        "size": "m2.4xlarge",
        "premium": 0.246
      }
    ],
    "medium": [
      {
        "size": "m2.2xlarge",
        "premium": 0.123
      },
      {
        "size": "c3.4xlarge",
        "premium": 0.210
      },
      {
        "size": "c3.2xlarge",
        "premium": 0.105
      }
    ]
  }
}
